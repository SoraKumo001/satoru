{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext", "WebWorker", "DOM"],
    "types": ["@cloudflare/workers-types", "react/jsx-runtime"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "include": ["src"]
}
