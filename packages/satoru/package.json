{
  "name": "satoru",
  "version": "1.0.0",
  "description": "HTML/CSS to SVG converter via Wasm (Skia + litehtml)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "workerd": {
        "types": "./dist/workerd.d.ts",
        "import": "./dist/workerd.js"
      },
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./workerd.js": {
      "types": "./dist/workerd.d.ts",
      "import": "./dist/workerd.js"
    },
    "./satoru.js": "./dist/satoru.js",
    "./satoru.wasm": "./dist/satoru.wasm"
  },
  "typesVersions": {
    "*": {
      "workerd": [
        "./dist/workerd.d.ts"
      ],
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}